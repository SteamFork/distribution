[AC][Display]
LockBeforeTurnOffDisplay=true

[AC][SuspendAndShutdown]
AutoSuspendAction=0
PowerButtonAction=1

[Battery][Display]
DimDisplayIdleTimeoutSec=300
LockBeforeTurnOffDisplay=true

[Battery][SuspendAndShutdown]
AutoSuspendAction=0
PowerButtonAction=0

[LowBattery][Display]
LockBeforeTurnOffDisplay=true
TurnOffDisplayIdleTimeoutSec=60

[LowBattery][SuspendAndShutdown]
PowerButtonAction=1
