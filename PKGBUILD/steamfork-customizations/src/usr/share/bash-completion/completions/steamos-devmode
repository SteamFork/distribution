_steamos-devmode()
{
    local cur prev words cword
    _init_completion || return

    local args
    _count_args
    if [[ "$args" -eq 1 ]]; then
        COMPREPLY=($(compgen -W "enable status" -- "$cur"))
    fi
} &&
complete -F _steamos-devmode steamos-devmode
